    "class Triangulo:\n",
    "    def __init__(self, base, altura):\n",
    "        self.base = base\n",
    "        self.altura = altura\n",
    "        \n",
    "    def calcularArea(self):\n",
    "        area = (self.base*self.altura)/2\n",
    "        return area"
